{
  "name": "nerd-stack-auth-starter",
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.0.7"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.18",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "studio": "bun --filter server studio",
    "migrate": "bun --filter server migrate",
    "generate": "bun --filter server generate"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
